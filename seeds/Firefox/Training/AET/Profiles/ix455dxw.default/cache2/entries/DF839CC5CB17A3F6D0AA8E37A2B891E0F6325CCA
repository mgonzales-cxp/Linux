<script type="text/javascript">	
   function saveResults(completion, campaign) {
   	
   
   	var myParams = new Hash();
   	myParams.set('referer', 'N/A');
   	myParams.set('complete', completion);
   
   			myParams.set('campaign', campaign);	
   		
   
   
   	myParams.set('referer', 'N/A');
   	myParams.set('visitorId', '01A201412111252071359177060');
   	myParams.set('sessionId', '01B201412111252071828610409');
   	myParams.set('remoteAddress', '10.1.1.212');
   	
   
   	new Ajax.Request("/AMSS/trackingLoggerAction.do?track", {
   		  method: 'get',
   		  parameters: myParams    
   	});	 
   	
   }
   
</script>
