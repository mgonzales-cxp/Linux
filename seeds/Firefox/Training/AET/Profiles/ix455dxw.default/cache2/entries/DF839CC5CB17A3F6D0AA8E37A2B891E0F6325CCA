jQuery(document).ready(function(){

    if(jQuery('#zipcode').val()==''){
      jQuery('#zipcode').val('Your ZIP');
    }

    if(jQuery('#firstname').val()==''){
      jQuery('#firstname').val('First name');
    }

    if(jQuery('#firstname').val()=='First'){
      jQuery('#firstname').val('First name');
    }
  
    jQuery('#zipcode').focus(function(){
      if(jQuery(this).val()=='Your ZIP'){
        jQuery(this).val('');
      }
    });

    jQuery('#zipcode').blur(function(){
      if(jQuery(this).val()==''){
        jQuery(this).val('Your ZIP');
      }
    });

    jQuery('#firstname').focus(function(){
      if(jQuery(this).val()=='First name'){
        jQuery(this).val('');
      }
    });

    jQuery('#firstname').blur(function(){
      if(jQuery(this).val()==''){
        jQuery(this).val('First name');
      }
    });

 });

//FORM HANDLING JS START
jQuery("#firstname, #zipcode, a.findPlans").keydown(function(event){
	//HANDLE RETURN HIT
    if(event.keyCode == 13){
    	jQuery("a.findPlans").click();
       //dofindplans(2014);
    }
});

function dofindplans(year) {
	//VALIDATE FORM
	//if ((jQuery("#firstname").val().length)&&(jQuery("#firstname").val() != "First name")) {
		//createCookie('GlobalName', jQuery("#firstname").val());
	//}

	if((jQuery('#firstname').val()!='')&&(jQuery('#firstname').val()!='First Name')&&(jQuery('#firstname').val()!='First name')){
      createCookie('GlobalName', jQuery("#firstname").val());
    }

	var enteredzip = jQuery("#zipcode").val();
	if ( (enteredzip.length != 5) || (enteredzip.search(/\D/) != -1) ) {
		alert("Please enter a valid 5 digit zip code.");
		return;
	} else {
		createCookie('GlobalZip', enteredzip);
	}
	//GO TO RESULTS PAGE
	jQuery("body, #findplans, input").css('cursor', 'wait');
	//SITECATALYST TRACKING
	//if ( !((jQuery.browser.msie == true) && (parseInt(jQuery.browser.version) == 6)) ) {
		s.linkTrackVars = 'events';
		s.linkTrackEvents = 'event12';
		s.events='event12';
		s.tl(true,'o','Hero - Explore Plans');
	//}
	//END SITECATALYST

	if(year==2014){
		createCookie('GlobalYear', "2014");
		window.location="foundplans_2014.jsp?"+Math.random();
	}else{
		createCookie('GlobalYear', "2015");
		window.location="foundplans-2015.jsp?"+Math.random();
	}
}
//FORM HANDLING JS END
//-->


//Load JQUERY
jQuery(document).ready(function() {
    jQuery.get('/includes/banner.xml', function(d){
        jQuery(d).find('banner').each(function(){  
            var banner = jQuery(this);   
            var title = banner.find("title").text();  
            var background = banner.find("background").text();  
            var cta = banner.find('cta').text();  
            var ctatitle = banner.find('ctatitle').text();  
            var url = banner.find('url').text();

            var cta1 = banner.find('cta1').text();
            var url1 = banner.find('url1').text();
            var cta2 = banner.find('cta2').text();
            var url2 = banner.find('url2').text();

            if(cta1!=""){
            	var newBanner = '<li class="withlinks" style="background: url(\''+background+'\')"><div class="tensor"><h1>'+title+'</h1><p><a href="'+url1+'">'+cta1+'</a></p><p><a href="'+url2+'">'+cta2+'</a></p><a href="'+url+'" onClick="linkCode(\'\',\'Hero - '+ cta +'\');" class="go">'+cta+'</a></div></li>';
            }else{
            	var yearSelectorClass = "";
				if(cta=="Find out"){
					yearSelectorClass = " year_selector_link";
	            }

            	var newBanner = '<li style="background: url(\''+background+'\')"><div class="tensor"><h1>'+title+'</h1><a href="'+url+'" onClick="linkCode(\'\',\'Hero - '+ cta +'\');" class="go' + yearSelectorClass + '">'+cta+'</a></div></li>';
            }

            jQuery('ul.banner').append(newBanner);
            init_YearSelector();
        });
        initRotatingBanner();
    });        
});

function initRotatingBanner(){
    jQuery('ul.banner') 
        .after('<div id="nav"></div>') 
        .cycle({ 
            fx:     'fade', 
            speed:  200, 
            timeout: 10000, 
            pager:  '#nav',
            pause: 1,

		    // create custom pager item
		    pagerAnchorBuilder: function(idx, slide) { 
		        return '<a class="step' + (idx+1)  + '" href="#">' + (idx+1) + '</a>'; 
		    } 
    });
}

